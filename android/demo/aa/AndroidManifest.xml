<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zskx.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.zskx.activity.MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zskx.activity.ReportActivity"
            android:screenOrientation="landscape"
            android:theme="@style/My_App_Theme" />
        <activity
            android:name="com.zskx.activity.ReportDetailActivity"
            android:screenOrientation="landscape"
            android:theme="@style/My_App_Theme" />
        
        <activity
            android:name="com.zskx.activity.MusicActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
        
        <activity
            android:name="com.zskx.activity.DecompressionActivity"
            
            android:screenOrientation="landscape"
            android:theme="@style/My_App_Theme" />
    </application>

</manifest>